{
  "name": "Flache",
  "version": "1.0",
  "manifest_version": 3,
  "minimum_chrome_version": "10.0",
  "devtools_page": "./pages/DevTools/index.html",
  "externally_connectable": {
    "matches": [
      "*://localhost/*"
    ]
  }, 
  "background": {
      "service_worker": "background.js" 
  },   
  "permissions" : [ 
    "storage",
    "idle",
    "*://localhost/*"
  ]
}